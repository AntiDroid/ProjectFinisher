studentenfolieseitn:

<- kursInfoRequest:
	userId
	kursId
-> kursInfo:
	kursName
	lehrerName

-> folienUpdate
	folienId
	fSatzName
	interaktiv(bool)
		if true{
		isHeatplot(bool)
		else{
		bereichList(Auswahlwahlbereich}}

<- Beantwortung:
		var bereichAntwort = {
				type : "bereichAntwort",
				userId : userId,
				kursId : kursId,
				bereichNr : bereichNr,
				posX : clickX,
				posY : clickY
			}
		var heatplotAntwort = {
				type : "heatplotAntwort",
				userId : userId,
				kursId : kursId,
				posX : clickX,
				posY : clickY
			};



lehrerfolienseite:
<- folienUpdateRequest
	folienId
	kursId
	sessionId
-> siehe folienUpdate

